&gpu {
	gpu_idle = <0>;
	dvfs_status = <1>;
	pll_rate = <456000>;
	independent_power = <0>;
	 markid-points = <
		0x0400 456
		0x1400 504
		0x1000 504
		0x2000 504
		0x0000 504
	 >;
	operating-points = <
		/* KHz   uV */
		456000 950000
		399000 950000
		228000 950000
	 >;
};

&cpu0 {
	cpu-supply = <&reg_dcdc1>;
};

/* audio dirver module -> audio codec */
&codec {
	/* note: power settings */
	avcc-external;
	avcc-supply	= <&reg_aldo4>;
	avcc-vol	= <1800000>;
	dvcc-external;
	dvcc-supply	= <&reg_cldo1>;
	dvcc-vol	= <1800000>;
	/* note: volume settings */
	adc-dig-vol-l	= <160>;
	adc-dig-vol-r	= <160>;
	mic1-vol	= <31>;
	mic2-vol	= <31>;
	dac-dig-vol	= <61>;
	dac-dig-vol-l	= <160>;
	dac-dig-vol-r	= <160>;
	lineout-vol	= <31>;
	hp-vol		= <7>;
	/* note: circuit settings */
	/* lineout-single; */
	pa-pin-max	= <1>;
	pa-pin-0	= <&pio PH 6 GPIO_ACTIVE_HIGH>;
	pa-pin-level-0	= <1>;
	pa-pin-msleep-0	= <0>;
	/* note: jack param -> codec */
	jack-det-level		= <0>;
	jack-det-threshold	= <8>;
	jack-det-debouce-time	= <250>;
	/* note: jack param -> extcon */
	/*
	extcon			= <&usb_power_supply>;
	jack-swpin-mic-sel	= <&pio PH 5 GPIO_ACTIVE_HIGH>;
	jack-swpin-hp-en	= <&pio PH 16 GPIO_ACTIVE_HIGH>;
	jack-swpin-hp-sel	= <&pio PH 15 GPIO_ACTIVE_HIGH>;
	jack-swmode-hp-off	= <0x00>;
	jack-swmode-hp-usb	= <0x01>;
	jack-swmode-hp-audio	= <0x10>;
	jack-det-level		= <0>;
	jack-det-threshold	= <8>;
	jack-det-debouce-time	= <250>;
	*/
	/* note: other settings */
	adc-delay-time	= <20>;
	tx-hub-en;
	rx-sync-en;
	status		= "okay";
};

&codec_plat {
	status		= "okay";
};

&codec_mach {
	soundcard-mach,jack;
	status		= "okay";
	soundcard-mach,cpu {
		sound-dai = <&codec_plat>;
	};
	soundcard-mach,codec {
		sound-dai = <&codec>;
	};
};

